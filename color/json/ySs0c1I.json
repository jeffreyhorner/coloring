    if(h.hour == 12) hstring = "12PM";
    if(h.hour == 0) hstring = "12AM";
    var height = Math.ceil(h.length/7)*120;
    hhtml += '<li style="height: '+(height-33)+'px;">'+hstring+'</li>';
});
$('#time_labels').html(hhtml);
